{
  "name": "@lyonbot/marked-vue",
  "version": "1.0.0",
  "description": "Efficient Markdown rendering in Vue. With incrementally update power!",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/lyonbot/marked-vue#readme",
  "bugs": {
    "url": "https://github.com/lyonbot/marked-vue/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lyonbot/marked-vue.git"
  },
  "author": "lyonbot <lyonbot@gmail.com>",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "packageManager": "pnpm@10.12.4",
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "playground": "vite",
    "test": "vitest",
    "typecheck": "vue-tsc --noEmit",
    "release": "bumpp && pnpm publish",
    "prepublishOnly": "pnpm run build"
  },
  "peerDependencies": {
    "marked": ">=4 <17",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@types/he": "^1.2.3",
    "@types/node": "^22.17.2",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/test-utils": "^2.4.6",
    "bumpp": "^10.2.3",
    "github-markdown-css": "^5.8.1",
    "happy-dom": "^17.6.3",
    "marked": "^16.1.2",
    "marked-emoji": "^2.0.1",
    "tsdown": "^0.11.13",
    "typescript": "^5.9.2",
    "vite": "npm:rolldown-vite@^7.1.2",
    "vitest": "^3.2.4",
    "vue": "^3.5.18",
    "vue-tsc": "^3.0.5"
  },
  "dependencies": {
    "he": "^1.2.0"
  }
}